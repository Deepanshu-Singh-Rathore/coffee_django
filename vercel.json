{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "coffee_project.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
